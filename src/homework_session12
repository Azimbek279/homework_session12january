-- 1. Баардык продукталарды(Products) жана информацияларды чыгар.
select * from Products join Product_Info PI on Products.id = PI.product_id;
-- 2. Информациясы бар болгон продукталарды гана чыгар.
select * from Product_Info  inner join Products p on Product_Info.product_id = p.id ;
-- 3. Информациясы жок(null) болгон продукталарды чыгар.
-- Responce: mende anday jok agai!
-- 4. Баардык продукталардын атын жана срогун чыгар.
select p.name,PI.expiration_date from Products p join Product_Info PI on p.id = PI.product_id;
-- 5. Баардык продукталарды жана анын мамлекеттери "М" менен башталган шаарларын чыгар.
select * from Products join Product_Info on country like 'M%';
-- 6. Баардык кардардын(Customers) аттарын жана кардарга тийешелуу карзинанын(Basket_терин) аттарын сорттоп чыгар.(asc,desc)
select * from Customers c join Basket b on c.id = b.customer_id
order by c.first_name;
-- 7. Кардардын аты жана кардардын продуктулары чыксын.
select c.first_name,p.name from Customers c join Basket B on c.id = B.customer_id join Product_Basket pb
    on B.id = pb.basket_id join Products p on pb.product_id = p.id;
-- 8. Кардардын аты, кардар алган продуктун аты, баасы, салмагы чыксын.
select c.first_name,p.name,p.price,p.weight from Customers c join Basket B on c.id = B.customer_id join Product_Basket pb
    on B.id = pb.basket_id join Products p on pb.product_id = p.id;
-- 9. Бир кардар эки окшош продукт алган болсо, кардардын аты,
--     кардар алган продукталарынын аты уникалдуу болуп баасы жана салмагы кошулуп чыгар.
select c.first_name, pg_ndistinct_in(p.name) ,p.price,p.weight from Customers c join Products p on c.id = p.id;
-- 10.Бир өлкөдө чыккан продукталдын баарын чыгар 'Portugal'.
select * from products join Product_Info PI on Products.id = PI.product_id
where country like 'Portugal';
-- 11.Кардарлардын баарын жана алган продукталарынын баасынын суммасын тегеректеп (округлить) чыгар.
select c.first_name,round(sum(P.price)) from Customers c join Basket B on c.id = B.customer_id join
    Product_Basket PB on B.id = PB.basket_id
join Products P on P.id = PB.product_id
group by c.first_name;
-- 12."2021" ден "2023" кө чейинки сроктогу продукталардын атын жана срокторун чыгар.
select c.name,PI.expiration_date from Products c join Product_Info PI on c.id = PI.product_id
where PI.expiration_date between  '2021-01-01' and '2023-12-31';
-- 13.Срогу "2022" болгон продукталардын шаарын , продукталардын атын жана баасын чыгар бирок баалары 0 болуп калсын.
select pi.country,P.name,P.price from Product_Info pi join Products P on P.id = pi.product_id
where pi.expiration_date between '2022-01-01' and '2022-12-31' and P.price IS NULL
group by  pi.country, P.name, P.price;
-- 14.Баардык кардарлардын продукталарынын сумассын чыгар.
select sum(P.price) from Customers c join Basket B on c.id = B.customer_id
    join Product_Basket PB on B.id = PB.basket_id join Products P on P.id = PB.product_id;
-- 15.Кардардын аты жана ал сатып алган продуктунун аты, тусу чыксын.
select c.first_name,P.name,PI.color from Customers c join Basket B on c.id = B.customer_id
    join Product_Basket PB on B.id = PB.basket_id
    join Products P on P.id = PB.product_id
    join Product_Info PI on P.id = PI.product_id;
-- 16.Баардык продуктулардын салмактарын чыгар.
select sum(weight) from Products;
-- 17.Кардарлардын email_дары "com" менен буткон кардарлардын аттары, корзинасынын аты чыксын.
select c.first_name,c.email,B.basket_name from Customers c join Basket B on c.id = B.customer_id
where c.email like '%com';
-- 18.Продуклардын атын, кыскача маалыматын(description), кардардын атын чыгар качан кардарлардын номери жок(null) болсо.
select p.name,p.description,c.first_name,c.phone_number from Customers c
    join Basket B on c.id = B.customer_id
    join Product_Basket PB on B.id = PB.basket_id
    join Products P on P.id = PB.product_id
where c.phone_number is null;
-- 19.Бардык продуктунун тустору жана тустору окшош продукталары чыксын.
select p.name,PI.color from Products p join Product_Info PI on p.id = PI.product_id
 order by PI.color;
-- 20.Баардык продукталардын мамлекти окшош болгон продукталардын атын жана анын кардарларын атын чыгар.
select p.name,PI.country,C.first_name from Products p join Product_Info PI
    on p.id = PI.product_id join Product_Basket PB on p.id = PB.product_id
    join Basket B on B.id = PB.basket_id join Customers C on C.id = B.customer_id
 order by PI.country;